n;\">\n\n          <div style=\"flex: 1; display: flex; align-items: center; justify-content: center; margin-top: -10px\">\n            <el-form style=\" width: 500px; font-size: 20px\" :model=\"form\"  :rules=\"rules\" ref=\"form\">\n              <div style=\"font-size: 500px; font-weight: bold; text-align: center; margin-bottom: 10px\"> </div>\n              <el-form-item prop=\"username\">\n                <el-input style=\"font-size: 20px\" v-model=\"form.username\" size=\"big\" placeholder=\"请输入账号\"  clearable>\n                  <template #prefix>\n                    <el-icon class=\"el-input__icon\"><User /></el-icon>\n                  </template>\n                </el-input>\n              </el-form-item>\n\n              <el-form-item prop=\"password\">\n                <el-input style=\"font-size: 20px\" v-model=\"form.password\"   size=\"big\"  placeholder=\"请输入密码\" clearable show-password>\n                  <template #prefix>\n                    <el-icon class=\"el-input__icon\" ><Lock /></el-icon>\n                  </template>\n                </el-input>\n              </el-form-item>\n\n              <el-form-item prop=\"code\">\n                <div style=\"display: flex\">\n                  <el-input placeholder=\"请输入验证码\" prefix-icon=\"el-icon-circle-check\" size=\"big\" style=\"flex: 1; font-size: 20px\" v-model=\"form.validCode\">\n                    <template #prefix>\n                      <el-icon class=\"el-input__icon\" ><Check/></el-icon>\n                    </template>\n                  </el-input>\n                  <div style=\"font-size: 20px;  flex: 1; height: 36px;margin-left: 13px\">\n\n                    <ValidCode @input=\"createValidCode\" style=\"width: 50%\"/>\n                  </div>\n                </div>\n              </el-form-item>\n\n              <el-form-item>\n                <el-button type=\"primary\" style=\"width: 100%; font-size: 20px\" @click=\"login\">登 录</el-button>\n              </el-form-item>\n\n              <div style=\"display: flex\">\n                <div style=\"flex: 1\">还没有账号？请 <span style=\"color: #0f9876; cursor: pointer\" @click=\"$router.push('/register')\">注册</span></div>\n                <div style=\"flex: 1; text-align: right\"><span style=\"color: #0f9876; cursor: pointer\" @click=\"handleForgetPass\">忘记密码</span></div>\n              </div>\n              <div style=\"font-size: 20px; font-weight: bold; text-align: center; margin-bottom: 20px\"> </div>\n            </el-form>\n          </div>\n        </div>\n\n\n\n\n      </div>\n    </div>\n  </div>\n\n</template>\n\n<script>\nimport request from \"../utils/request\";\nimport {ElMessage} from \"element-plus\";\nimport ValidCode from \"../components/Validate\";\nimport {Lock, User} from \"@element-plus/icons\";\n\n\n\nexport default {\n  name: \"Login\",\n  components:{\n    User,\n    Lock,\n    ValidCode\n  },\n  data() {\n    return {\n      validCode: '',//通过valicode获取的验证码\n      form: {},\n      rules: {\n        username: [\n          {\n            required: true,\n            message: '请输入用户名',\n            trigger: 'blur',\n          }\n        ],\n        password: [\n          {\n            required: true,\n            message: '请输入密码',\n            trigger: 'blur',\n          }\n        ]\n\n      },\n      dialogVisible: false,  // 控制弹框显示与隐藏\n      fileList: []           // 保存文件列表\n\n    }\n  },\n  methods: {\n    createValidCode(data){\n      this.validCode = data\n    },\n    handleForgetPass(){\n      // ElMessage.error(\"请联系校团委管理员\")\n      this.$alert('请联系系统管理员', '忘记密码', {\n        confirmButtonText: '确定',\n      });\n\n    },\n    login(){\n      this.$refs['form'].validate((valid) => {\n        if (valid) {\n          if (!this.form.validCode) {\n            ElMessage.error(\"请填写验证码\")\n            return\n          }\n          if(this.form.validCode.toLowerCase() !== this.validCode.toLowerCase()) {\n            ElMessage.error(\"验证码错误\")\n            return\n          }\n\n          request.post(\"user/login\", this.form).then(res => {\n            if (res.code == 0) {\n              ElMessage.success(\"登录成功\")\n              sessionStorage.setItem(\"user\",JSON.stringify(res.data))//缓存用户信息\n\n              this.$router.push(\"/web/nftview\")\n            } else {\n              ElMessage.error(res.msg)\n            }\n          })\n        }\n      })\n\n    }\n  }\n}\n\n</script>\n\n<style scoped>\n.login-wrap {\n  position: relative;\n  width: 100%;\n  height: 100%;\n  background: #ffffff;\n  background-image: url(\"../assets/img/10.png\")!important;\n\n  background-size: cover;\n  background-repeat: no-repeat;\n  background-position: center;\n}\n</style>"],"mappings":";;OAOeA,UAAyB;;EANjCC,KAA2G,EAA3G;IAAA;IAAA;IAAA;IAAA;IAAA;EAAA,CAA2G;EAACC,KAAK,EAAC;;;EAEhHD,KAAgG,EAAhG;IAAA;IAAA;IAAA;IAAA;IAAA;EAAA;AAAgG;;EAC9FA,KAAkE,EAAlE;IAAA;IAAA;IAAA;EAAA;AAAkE;;EAOhEA,KAAmI,EAAnI;IAAA;IAAA;IAAA;IAAA;IAAA;IAAA;EAAA;AAAmI;;EAEjIA,KAA+F,EAA/F;IAAA;IAAA;IAAA;IAAA;IAAA;EAAA;AAA+F;;EAoBzFA,KAAqB,EAArB;IAAA;EAAA;AAAqB;;EAMnBA,KAAiE,EAAjE;IAAA;IAAA;IAAA;IAAA;EAAA;AAAiE;;EAWrEA,KAAqB,EAArB;IAAA;EAAA;AAAqB;;EACnBA,KAAe,EAAf;IAAA;EAAA;AAAe;;EACfA,KAAkC,EAAlC;IAAA;IAAA;EAAA;AAAkC;;;;;;;;;;;uBAnDrDE,mBAAA,CA+DM,OA/DNC,UA+DM,GA7DJC,mBAAA,CA4DM,OA5DNC,UA4DM,GA3DJD,mBAAA,CA0DM,OA1DNE,UA0DM,G,0BAxDJF,mBAAA,CAEM;IAFDJ,KAAe,EAAf;MAAA;IAAA;EAAe,IAClBI,mBAAA,CAA0E;IAArEG,GAAyB,EAAzBR,UAAyB;IAACS,GAAG,EAAC,EAAE;IAACR,KAAmC,EAAnC;MAAA;MAAA;IAAA;uDAExCI,mBAAA,CACM;IADDJ,KAAoB,EAApB;MAAA;IAAA;EAAoB,6BAEzBI,mBAAA,CA8CM,OA9CNK,UA8CM,GA5CJL,mBAAA,CA2CM,OA3CNM,UA2CM,GA1CJC,YAAA,CAyCUC,kBAAA;IAzCDZ,KAAsC,EAAtC;MAAA;MAAA;IAAA,CAAsC;IAAEa,KAAK,EAAEC,KAAA,CAAAC,IAAI;IAAIC,KAAK,EAAEF,KAAA,CAAAE,KAAK;IAAEC,GAAG,EAAC;;sBAChF,MAAiG,C,0BAAjGb,mBAAA,CAAiG;MAA5FJ,KAAoF,EAApF;QAAA;QAAA;QAAA;QAAA;MAAA;IAAoF,6BACzFW,YAAA,CAMeO,uBAAA;MANDC,IAAI,EAAC;IAAU;wBAC3B,MAIW,CAJXR,YAAA,CAIWS,mBAAA;QAJDpB,KAAuB,EAAvB;UAAA;QAAA,CAAuB;oBAAUc,KAAA,CAAAC,IAAI,CAACM,QAAQ;mEAAbP,KAAA,CAA